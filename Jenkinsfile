pipeline {
	stages {
		stage('Test') {
			steps {
				echo "This"
				echo "is"
				echo "test stage"
			}
		}
		stage('Build') {
			steps {
				echo "This"
				echo "is"
				echo "build stage"
			}
		}		
		stage('Deploy') {
			steps {
				echo "This"
				echo "is"
				echo "deploy stage"
			}
		}			
	}
}
